<div class="jumbotron jumbotron-fluid text-right">
  <div class="container">
    {{#each trendings as |trending|}}
      <h2 class="display-3">{{trending.name}}</h2>
      <div class="col-md-8">
        <h5>{{trending.user.name}}</h5>
        <small>{{trending.description}}</small>
      </div>
    {{/each}}
  </div>
</div>
